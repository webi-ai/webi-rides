{
  "version": 1,
  "canisters": {
    "token": {
      "package": "token",
      "candid": "token/token.did",
      "type": "rust"
    },
    "ledger": {
      "type": "custom",
      "wasm": "ledger.wasm",
      "candid": "ledger.public.did"
    }
  },
  "defaults": {
    "build": {
      "packtool": ""
    },
    "defaults":{
      "replica": {
        "subnet_type":"system"
      }
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    }
  }
}
